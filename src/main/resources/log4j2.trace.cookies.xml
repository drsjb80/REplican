<?xml version="1.0" encoding="UTF-8"?>
<!-- FATAL, ERROR, DEBUG, WARN, INFO, TRACE -->
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%level %thread %message %location%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="edu.msudenver.cs.replican.Cookie" level="TRACE"/>
        <Logger name="edu.msudenver.cs.replican.Cookies" level="TRACE"/>
        <Root level="warn">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>

